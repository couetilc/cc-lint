{
  "name": "cc-lint",
  "version": "0.2.0",
  "description": "ESLint wrapper for personal projects",
  "files": [
    "cli.js",
    ".eslintrc.*"
  ],
  "bin": {
    "cc-lint": "cli.js"
  },
  "scripts": {
    "test": "./cli.js cli.js && ./cli.js tests/lint.js && ./cli.js --react tests/react.js",
    "test:cli": "./cli.js cli.js",
    "test:lint": "./cli.js tests/lint.js",
    "test:react": "./cli.js --react tests/react.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/couetilc/cc-lint.git"
  },
  "keywords": [
    "eslint",
    "cc-lint",
    "cc"
  ],
  "author": "Connor Couetil",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/couetilc/cc-lint/issues"
  },
  "homepage": "https://github.com/couetilc/cc-lint#readme",
  "dependencies": {
    "eslint": "^7.24.0",
    "eslint-config-cc-lint": "^1.4.0",
    "eslint-config-cc-react": "^1.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0"
  },
  "devDependencies": {}
}
